{
  "name": "@wasc/tools",
  "version": "5.8.4",
  "author": "Wasc",
  "engines": {
    "node": ">=10.0.0"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wasc-io/tools"
  },
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint -c ./config/.eslintrc.js .",
    "format": "node ./bin format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*{.js,jsx,ts,tsx}": "eslint -c ./config/.eslintrc.js",
    "*.{js,json,graphql,md,html,ts,tsx}": "npm run format --"
  },
  "bin": {
    "wasc-tools": "./bin/index.js"
  },
  "peerDependencies": {
    "source-map-support": ">0.0.0"
  },
  "dependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-proposal-optional-chaining": "7.11.0",
    "@babel/plugin-transform-react-jsx-source": "7.10.5",
    "@babel/preset-env": "7.11.5",
    "awesome-typescript-loader": "5.2.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "babel-plugin-inline-import": "3.0.0",
    "babel-plugin-syntax-trailing-function-commas": "6.22.0",
    "compression-webpack-plugin": "4.0.1",
    "copy-webpack-plugin": "6.1.0",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-no-secrets": "0.6.8",
    "eslint-plugin-optimize-regex": "1.2.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "execa": "4.0.3",
    "globby": "11.0.1",
    "graphql": "15.3.0",
    "graphql-import-loader": "0.2.1",
    "graphql-tag": "2.11.0",
    "html-loader": "1.3.0",
    "jest": "26.4.0",
    "markdown-loader": "5.1.0",
    "marked": "1.1.1",
    "node-loader": "1.0.1",
    "nodemon-webpack-plugin": "4.3.2",
    "prettier": "2.1.2",
    "progress-bar-webpack-plugin": "2.1.0",
    "sloc": "0.2.1",
    "then-fs": "2.0.0",
    "typescript": "3.9.7",
    "webpack": "4.44.1",
    "webpack-node-externals": "2.5.2",
    "yargs": "15.4.1"
  },
  "devDependencies": {
    "husky": "4.3.0",
    "lint-staged": "10.3.0"
  }
}
